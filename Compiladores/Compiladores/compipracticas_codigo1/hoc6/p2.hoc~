proc fibo(){
	a=1
	b=1
        cta=0
	while(cta < $1){
           fi=a+b
	   a=b
           b=fi
	   print fi
           cta= cta+1
	}
}

func fac(){
	if($1==0) return 1 else return $1*fac($1-1)
}
func fac(){
	if($1 < 2) return 1 else return $1*fac($1-1)
} 
